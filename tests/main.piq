fn main(int i, float f) {
    int foo = -i + @index;
    float bar = f <? 42.0;
    bool baz = !true;

    if (baz) {
        bar = bar + f;
    } else if (bar < 5.0) {
        bar = 1.0;
    } else {
        bar = 2.0;
    }

    while (baz) {
        bar = bar + f;
    }

    bar = bar + f;
    return;
}
